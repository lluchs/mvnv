{{! Lists scores in a table. }}
<table class="scores table table-hover">
	<thead>
		<tr>
			<th data-sort="id">ID</th>
			<th data-sort="string" class="default">Titel</th>
			<th data-sort="string">Komponist</th>
		</tr>
	</thead>
	<tbody>
		{{#scores}}
			<tr>
				<td><a href="/racks/by/score/{{id}}">{{id}}</a></td>
				<td><a href="/scores/{{_id}}"><b>{{title}}</b></a></td>
				<td>{{composer}}</td>
			</tr>
		{{/scores}}
	</tbody>
</table>
